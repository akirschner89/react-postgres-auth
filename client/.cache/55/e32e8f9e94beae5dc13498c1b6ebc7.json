e":"src/Auth.js","original":{"line":48,"column":11}},{"generated":{"line":80,"column":6},"source":"src/Auth.js","original":{"line":48,"column":4}},{"generated":{"line":81,"column":0},"source":"src/Auth.js","original":{"line":59,"column":3}},{"name":"Auth","generated":{"line":84,"column":15},"source":"src/Auth.js","original":{"line":63,"column":15}}],"sources":{"src/Auth.js":"import history from './history';\nimport { CONNECTION } from '../config'\n\n\nclass Auth {\n  loggedIn = false;\n\n  authenticate = (username, password, type) => {\n    fetch(`${CONNECTION}/user/${type}`, {\n      method: 'POST',\n      credentials: 'include',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({ username, password })\n    }).then(response => response.json())\n      .then(json => {\n        if (json.type === 'error') {\n          alert(json.msg)\n        }\n        else {\n          this.loggedIn = true\n          history.replace('/callback')\n        }\n      })\n\n  }\n\n  signup = (username, password) => {\n    this.authenticate(username, password, 'new')\n  }\n\n\n  login = (username, password) => {\n    this.authenticate(username, password, 'login')\n  }\n\n\n  logout = () => {\n    fetch(`${CONNECTION}/user/logout`, {\n      credentials: 'include'\n    }).then(response => response.json())\n      .then(() => {\n          this.loggedIn = false\n          history.replace('/callback')\n      })\n  }\n\n  checkAuthentication = () => {\n    return new Promise((resolve, reject) => {\n      fetch(`${CONNECTION}/user/authenticated`, {\n        credentials: 'include'\n      }).then(response => response.json())\n      .then(json => {\n        if (json.authenticated) {\n          this.loggedIn = true\n        }\n        resolve()\n      })\n    })\n  }\n\n}\n\nexport default Auth;"},"lineCount":null}},"hash":"a22209f2b2e3f5111c8c44379fd8e0d2","cacheData":{"env":{}}}